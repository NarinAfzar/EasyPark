@model IEnumerable<EasyPark.WebUI.Areas.ParkMan.ViewModels.Ticket.ParkedListViewModel>

@{
    ViewBag.Title = "Parked list";
}
<div class="box box-primary">
    <div class="box-header with-border">
        <h3 class="box-title">Parked List</h3>
        <div class="box-tools pull-right">
            <!-- Buttons, labels, and many other things can be placed here! -->
            <!-- Here is a label for example -->
            <span class="fa fa-navicon"></span>
        </div><!-- /.box-tools -->
    </div><!-- /.box-header -->

    <div class="box-body">
<section id="no-more-tables">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.TicketNumber)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Plate)
                </th>
                <th class="negligible">
                    @Html.DisplayNameFor(model => model.Description)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DepartureTime)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ParkDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ParkingSlot)
                </th>
                <th class="negligible">
                    @Html.DisplayNameFor(model => model.RequestTime)
                </th>
                <th></th>
            </tr>
        </thead>
        @foreach (var item in Model)
        {
            <tbody>
                <tr>
                    <td data-title="@Html.DisplayNameFor(model => model.TicketNumber)">
                        @Html.DisplayFor(modelItem => item.TicketNumber)
                    </td>
                    <td data-title="@Html.DisplayNameFor(model => model.Plate)">
                        @Html.DisplayFor(modelItem => item.Plate)
                    </td>
                    <td data-title="@Html.DisplayNameFor(model => model.Description)" class="negligible">
                        @Html.DisplayFor(modelItem => item.Description)
                    </td>
                    <td data-title="@Html.DisplayNameFor(model => model.DepartureTime)">
                        @Html.DisplayFor(modelItem => item.DepartureTime)
                    </td>
                    <td data-title="@Html.DisplayNameFor(model => model.ParkDate)">
                        @Html.DisplayFor(modelItem => item.ParkDate)
                    </td>
                    <td data-title="@Html.DisplayNameFor(model => model.ParkingSlot)">
                        @Html.DisplayFor(modelItem => item.ParkingSlot)
                    </td>
                    <td data-title="@Html.DisplayNameFor(model => model.RequestTime)" class="negligible">
                        @Html.DisplayFor(modelItem => item.RequestTime)
                    </td>
                    <td data-title="Action">
                        @Html.ActionLink("Return", "Return", new { id = item.ID }, new { @class = "btn btn-default" })
                    </td>
                </tr>
            </tbody>
        }
    </table>
</section>
    </div><!-- /.box-body -->
</div><!-- /.box -->


