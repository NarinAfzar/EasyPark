@model IEnumerable<EasyPark.WebUI.Areas.ParkMan.ViewModels.Ticket.ListItemViewModel>

@{
    ViewBag.Title = "Dispatched list";
}

<div class="box box-primary">
    <div class="box-header with-border">
        <h3 class="box-title">Dispatched cars list</h3>
        <div class="box-tools pull-right">
            <!-- Buttons, labels, and many other things can be placed here! -->
            <!-- Here is a label for example -->
            <span class="fa fa-sign-in"></span>
        </div><!-- /.box-tools -->
    </div><!-- /.box-header -->

    <div class="box-body">
        <section id="no-more-tables">

            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.TicketNumber)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Description)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.LicensePlate)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.DepartureTime)
                        </th>
                        <th></th>
                    </tr>
                </thead>
                @foreach (var item in Model)
                {
                    <tbody>
                        <tr>
                            <td data-title="@Html.DisplayNameFor(model => model.TicketNumber)">
                                @Html.DisplayFor(modelItem => item.TicketNumber)
                            </td>
                            <td data-title="@Html.DisplayNameFor(model => model.Description)">
                                @Html.DisplayFor(modelItem => item.Description)
                            </td>
                            <td data-title="@Html.DisplayNameFor(model => model.LicensePlate)">
                                @Html.DisplayFor(modelItem => item.LicensePlate)
                            </td>
                            <td data-title="@Html.DisplayNameFor(model => model.DepartureTime)">
                                @Html.DisplayFor(modelItem => item.DepartureTime)
                            </td>
                            <td data-title="Action">
                                @Html.ActionLink("Park", "Park", new { id = item.ID }, new { @class = "btn btn-default" })
                            </td>
                        </tr>
                    </tbody>
                }
            </table>
        </section>
    </div><!-- /.box-body -->
</div><!-- /.box -->



